<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".CalendarFragment">

    <ImageView
        android:id="@+id/header_image"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:src="@drawable/gw_orlando_logo_1"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="-125dp"
        android:contentDescription="Green Works Orlando logo"/>

    <WebView
        android:id="@+id/webview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/header_image"
        android:layout_above="@+id/city_of_orlando_image" />

    <ImageView
        android:id="@+id/city_of_orlando_image"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:src="@drawable/city_of_orlando_logo_new_1"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="65dp"
        android:layout_marginStart="-20dp"
        android:contentDescription="City of Orlando logo"/>

    <TextView
        android:id="@+id/city_of_orlando_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/city_of_orlando_image"
        android:layout_alignBottom="@+id/city_of_orlando_image"
        android:text="Â© 2024 City of Orlando"
        android:layout_marginStart="-25dp"
        android:textColor="#FF000000"/>

</RelativeLayout>
